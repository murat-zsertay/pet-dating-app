{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@sentry/react": "^7.40.0",
    "@sentry/tracing": "^7.40.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cypress": "^11.2.0",
    "http-proxy-middleware": "2.0.6",
    "pm2": "^5.2.2",
